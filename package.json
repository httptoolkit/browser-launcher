{
  "name": "@httptoolkit/browser-launcher",
  "version": "2.3.0",
  "description": "Detect, launch and stop browser versions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "browser-launcher": "./dist/bin.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "prebuild": "npm run clean",
    "test": "mocha --require tsx 'test/**/*.test.js'",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "res"
  ],
  "directories": {
    "example": "example",
    "res": "res"
  },
  "dependencies": {
    "@httptoolkit/osx-find-executable": "^2.0.1",
    "headless": "^1.0.0",
    "lodash": "^4.17.21",
    "simple-plist": "^1.0.0",
    "win-detect-browsers": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/httptoolkit/browser-launcher.git"
  },
  "homepage": "https://github.com/httptoolkit/browser-launcher",
  "keywords": [
    "browser",
    "headless",
    "phantom",
    "chrome",
    "firefox",
    "chromium",
    "safari",
    "ie",
    "opera",
    "osx",
    "windows"
  ],
  "author": "Tim Perry <tim@httptoolkit.com>",
  "contributors": [
    "James Halliday <mail@substack.net> (http://substack.net)",
    "CKSource (http://cksource.com/)",
    "benderjs",
    "mitchhentges"
  ],
  "license": "MIT",
  "engine": {
    "node": ">=22"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.7.0",
    "chai": "^6.2.0",
    "mocha": "^11.7.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
